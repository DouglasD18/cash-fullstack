    "@types/node": "^18.11.17",
    "@types/sequelize": "^4.28.14",
    "jest": "^29.3.1",
    "mysql2": "^2.3.3",
    "nodemon": "^2.0.20",
    "sequelize-cli": "^6.5.2",
    "ts-jest": "^29.0.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
